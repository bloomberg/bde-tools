#!/bin/ksh

# Arch group source code differential check in
# Displays a diff showing the changes immediately before checking
# each file in.  The on-screen display can be used to cut and
# paste into the checkin comments.

PATH=/bbsrc/bin/prod/bin/robo:$PATH

for i in "$@"
do
    aco -p $i | diff -u - $i
    aci -u $i
done
