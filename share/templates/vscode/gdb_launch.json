{
    "name": "(gdb) ",
    "type": "cppdbg",
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description": "Set command line arguments",
            "text": "-gdb-set args ${input:args}",
            "ignoreFailures": true
        },
        {
            "text": "source ${workspaceFolder}/.vscode/init-bde-pretty-printers"
        }
    ]
}