{
    "name": "(UDB) ",
    "type": "cppdbg",
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description": "Set command line arguments",
            "text": "-gdb-set args ${input:args}",
            "ignoreFailures": true
        },
        {
            "text": "source ${workspaceFolder}/.vscode/init-bde-pretty-printers"
        }
    ],
    "miDebuggerPath": "udb",
    "miDebuggerArgs": "--max-event-log-size 4G",
    "logging": {
        "trace": false,
        "traceResponse": false,
        "engineLogging": false
    },
    "udb": "live",
    "timezone": ""
}