[% USE String -%]
[% USE Dumper -%]
[%#
     Some usefull vars & macros
-%]
[% SET SERVICE = String.new(opts.serviceName).upper -%]
[% SET service = String.new(SERVICE).lower -%]
[% SET PKG = String.new(opts.package).upper -%]
[% SET pkg = String.new(PKG).lower -%]
[% SET namespace = pkg -%]
[% SET MSGPKG = String.new(opts.msgLibrary).upper -%]
[% SET msgpkg = String.new(MSGPKG).lower -%]
[% SET msgnamespace = msgpkg -%]
[% SET MSGNS = (pkg == msgpkg) ? '' : String.new(msgpkg).append('::') -%]
[% MACRO requesttype  GET svc.requestType  | lower -%]
[% MACRO requestType  GET svc.requestType  | mixedMixed -%]
[% MACRO RequestType  GET svc.requestType  | MixedMixed -%]
[% MACRO responsetype GET svc.responseType | lower -%]
[% MACRO responseType GET svc.responseType | mixedMixed -%]
[% MACRO ResponseType GET svc.responseType | MixedMixed -%]
[% USE year = date(format => '%Y') -%]
